    LV_TABLE_PART_CELL2,  /*   number of styles specified by LV_TABLE_CELL_STYLE_CNT            */
    LV_TABLE_PART_CELL3,
    LV_TABLE_PART_CELL4,  /* CELL 5-16 are not needed to be defined, the values in this enum
